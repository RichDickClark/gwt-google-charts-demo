package edu.mit.ll.div4.grp42.client;

public class EllipsoidModel {
	
	/**
	 * Calculate the local East, North and UP unit vectors, in ECEF coordinates, 
	 * at a location given in geodetic latitude and longitude.    
	 * @param latitude_degrees
	 * @param longitude_degrees
	 * @param[out] local_east
	 * @param[out] local_north
	 * @param[out] local_up
	 * 
	 * It is required that local_east, local_north and local_up already have field
	 * value initialized to allocated arrays
	 */
	public static void localEastNorthUp(double latitude_degrees, double longitude_degrees,
			Vector3d local_east, Vector3d local_north, Vector3d local_up ) {
		double sinlat = Math.sin( UnitConversion.convertDegreesToRadians(latitude_degrees) );
		double coslat = Math.cos( UnitConversion.convertDegreesToRadians(latitude_degrees) );
		double sinlon = Math.sin( UnitConversion.convertDegreesToRadians(longitude_degrees) );
		double coslon = Math.cos( UnitConversion.convertDegreesToRadians(longitude_degrees) );
		local_east.value[0] = -sinlon;
		local_east.value[1] = coslon;
		local_east.value[2] = 0.0;
		local_north.value[0] = 
		
		local_north.value = new double[3];
		local_up.value = new double[3];
		
		
		
		
	}

	
}
