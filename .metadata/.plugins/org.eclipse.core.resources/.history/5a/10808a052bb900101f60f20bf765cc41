package com.flyingsloth.financial.data;

public class DateDouble implements java.io.Serializable {
	public java.util.Date date;
	public double value;
	
	public DateDouble( java.util.Date date, double value ){
		this.date = date;
		this.value = value;
	}
	
	@Override
	public String toString() {
		return "(" + this.date.toString() + ", " + Double.toString(this.value) + ")";
	}
	
}
